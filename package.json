{
  "name": "jq",
  "version": "0.0.1",
  "description": "",
  "author": "vitprog@gmail.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clear": "rimraf dist",
    "build:server": "rimraf dist/server && rimraf dist/common && nest build",
    "build:client": "rimraf dist/cleant && webpack",
    "format": "prettier --write \"src/**/*.ts\" \"src/server/test/**/*.ts\"",
    "start:server": "nest start",
    "start:server:dev": "nest start --watch",
    "start:server:debug": "nest start --debug --watch",
    "start:server:prod": "node dist/main",
    "server": "cross-env RUN_MODE=ts-node ts-node --project tsconfig.build.json ./src/server/main.ts",
    "start:client": "webpack serve",
    "start:client:prod": "echo 'ToDo'",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config src/server/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/typeorm": "^7.1.5",
    "@types/react-router-dom": "^5.1.6",
    "@types/styled-components": "^5.1.4",
    "cache-manager": "^3.4.0",
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1",
    "nestjs-typeorm-paginate": "^2.2.3",
    "react-router-dom": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "styled-components": "^5.2.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/cache-manager": "^2.10.3",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.1",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "copy-webpack-plugin": "^6.3.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typeorm-model-generator": "^0.4.3",
    "typescript": "^4.0.5",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
